variable "aws_region" {
  description = "AWS provider default region"
  type        = string
  default     = "eu-central-1"
}

variable "aws_access_key" {
  description = "AWS provider access key"
  type        = string
  default     = "AKIAYEKP5ZEFFJDWCAPB"
}

variable "aws_secret_key" {
  description = "AWS provider secret access key"
  type        = string
}

